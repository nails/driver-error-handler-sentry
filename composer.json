{
    "name": "nailsapp/driver-error-handler-sentry",
    "keywords": ["Nails", "Shed Collective", "CodeIgniter", "ErrorHandler", "Driver", "Sentry"],
    "description": "This driver provides support for sentry.io error handling in Nails",
    "license": "MIT",
    "authors":
    [
        {
            "name": "Pablo de la Pena",
            "email": "p@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        }
    ],
    "support":
    {
        "email": "hello@nailsapp.co.uk"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload":
    {
        "psr-4": {"Nails\\Common\\ErrorHandler\\": "src/"}
    },
    "require-dev":
    {
        "phpunit/phpunit": "4.*"
    },
    "extra": {
        "nails": {
            "name": "Default",
            "type": "driver",
            "forModule": "nailsapp/common",
            "subType": "ErrorHandler",
            "data": {
                "namespace": "Nails\\Common\\ErrorHandler\\",
                "class": "Sentry"
            }
        }
    }
}